<h1>Student Interns</h1>

<table border="1">
  <tr>
    <th>
      First Name
    </th>

    <th>
      Last Name
    </th>

    <th>
      Uinversity
    </th>

    <th>
      Year
    </th>

    <th>
      Major
    </th>

    <th>
      Contact
    </th>

  </tr>

 <% @intern.each do |student| %>
  <tr>
    <td>
      <%= student.firstname %>
    </td>

    <td>
      <%= student.lastname %>
    </td>

    <td>
      <%= student.university %>
    </td>

    <td>
      <%= student.year %>
    </td>

    <td>
      <%= student.major %>
    </td>

    <td>
      <%= student.email %>
    </td>

  </tr>
  
  <% end %>
</table>
